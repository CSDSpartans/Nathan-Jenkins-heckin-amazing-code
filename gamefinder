def gamefinder():
  gameswon = int(input("How many games have you won? "))
  gameslost = int(input("How many games have you lost? "))
  gamesplayed = gameswon + gameslost
  winpercentage= float(((gamesplayed)/(gameswon))*100)
  return str(winpercentage)
  
